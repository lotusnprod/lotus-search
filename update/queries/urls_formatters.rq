    wd:P6689  # STRUCTURE MassBank
    wd:P7715  # TAXON     WFO
    wd:P7746  # STRUCTURE NPAtlas
    wd:P8494  # STRUCTURE DSSTox compound
    wd:P8533  # STRUCTURE SMARTS
